{
  "name": "donesuite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:dev": "chmod +x deploy.sh && ./deploy.sh dev",
    "deploy:prod": "chmod +x deploy.sh && ./deploy.sh prod",
    "setup:aws": "npm run setup:params && npm run setup:oauth",
    "setup:params": "echo 'Setting up AWS Parameters...' && node scripts/setup-params.js",
    "setup:oauth": "echo 'Please configure OAuth providers manually'",
    "postinstall": "if [ -f node_modules/.bin/vite ]; then chmod +x node_modules/.bin/vite; fi"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "axios": "^1.6.7",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.3",
    "serverless-dotenv-plugin": "^6.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "hackathon",
    "management",
    "platform",
    "react",
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "VENKEY STACKS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/venkateshstacks/donesuite-platform"
  },
  "bugs": {
    "url": "https://github.com/venkateshstacks/donesuite-platform/issues"
  },
  "homepage": "https://github.com/venkateshstacks/donesuite-platform#readme",
  "description": "DoneSuite - Modern task management dashboard by VENKEY STACKS"
}
